import java.util.Scanner;
class SumOfDigit
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter any no. to print its sum of all digit :");
		int x=sc.nextInt(),sum=0,d;
		for(int i=x;i>0;)
		{
			d=i%10;
			sum=sum+d;
			i=i/10;
		}
		System.out.println(sum);
	}
}

