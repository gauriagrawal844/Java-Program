import java.util.Scanner;
class PrintChar
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter any String :");
		String str=sc.nextLine();
		char ch;

		for(int i=0;i<str.length();i++)
		{
			ch=str.charAt(i);
			System.out.println(ch);
		}
	}
}
