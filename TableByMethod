import java.util.Scanner;
class TableByMethod
{
	static void Table(int x)
	{
		for (int i=1;i<=10;i++)
		System.out.println(x+"*"+i+"="+x*i);
	}
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number :");
		int x=sc.nextInt();
		Table(x);
	}
}

