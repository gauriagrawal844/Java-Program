/*
1
11
111
1111
*/




import java.util.Scanner;
class Pattern9
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter valur of n :");
		int n=sc.nextInt();
		for(int i=1;i<=n;i++)
		{
			for(int j=1;j<=i;j++)
			{
				System.out.print("1");
			}
			System.out.println();
		}
	}
}
