//Fibonacci series.
import java.util.Scanner;
class FibonacciSeries
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter any term till you want to print fibonnaci series :");
		int x=sc.nextInt(),a=0,b=1,c;
		System.out.print(a+","+b);
		for(int i=2;i<x;i++)
		{
			c=a+b;
			System.out.print(","+c);
			a=b;
			b=c;
		}
	}
}
