import java.util.Scanner;
class PrimeByMethod
{
	static boolean Prime(int x)
	{
		int c=0;
		boolean b=false;
		for(int i=1;i<=x;i++)
		{
			if(x%i==0)
			c++;
		}
			if(c==2)
			b=true;


		return b;
	}
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter any Number : ");
		int x=sc.nextInt();
		System.out.println(x+" is prime : "+Prime(x));
	}
}
